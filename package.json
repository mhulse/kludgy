{
  "name": "kludgy",
  "version": "0.1.0",
  "description": "Random Google Street View Desktop Wallpaper for macOS",
  "author": "Michael Hulse",
  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mhulse/kludgy.git"
  },
  "main": "kludgy",
  "scripts": {
    "getCoords": "source ./kludgy.sh; getCoords",
    "makePano": "source ./kludgy.sh; makePano",
