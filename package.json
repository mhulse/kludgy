  "scripts": {
    "latlng": "LATLNG=\"$(node .)\" && echo $LATLNG",
    "basic": "LATLNG=\"$(node .)\" && extract-streetview \"$LATLNG\" --zoom max > ~/Desktop/panorama.png",
    "_pano": "f() { export EVENT_NOKQUEUE=1; LATLNG=\"$(node .)\" && extract-streetview \"$LATLNG\" --zoom max > \"${1}pano-${LATLNG}.png\" && xxxxxxx; }; f",
    "pano": "npm run _pano",
    "_example": "f() { echo \"Hello ${1}!\" && echo \"Bye ${2}!\"; }; f",
    "example": "npm run _example -- world stranger"
  },
  "main": "index.js",
